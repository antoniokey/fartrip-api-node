{
  "name": "fartrip-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "start:dev": "ts-node-dev ./bin/www.ts",
    "start:debug": "ts-node-dev --inspect -- ./bin/www.ts",
    "start:prod": "npm run build && cd ./dist/bin && node www.js",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:drop": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "pretest": "npm run db:drop -- --env test && npm run db:migrate -- --env test && npm run db:seed -- --env test",
    "test": "npm run pretest && jest --passWithNoTests --detectOpenHandles"
  },
